@page
@model LMS.Admin.Views.Book.IndexModel
@{
   
}
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="template-demo">
                    <button type="button" class="btn btn-social-icon-text btn-google" id="btnAddEditInventoryPopUp"
                            data-bs-toggle="modal" data-bs-target="#dvAddEditBookPopUp">
                        <i class="mdi mdi-plus"></i>Add New Book
                    </button>
                </div>
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                
                                <th>Accession Number</th>
                                <th>Accessor Date</th>
                                <th>Bar Code</th>
                                <th>Title</th>
                                <th>Author</th>
                                <th>Year</th>
                                <th>Edition</th>
                                <th>Publisher</th>
                                <th>Publisher Location</th>
                                <th>Volume</th>
                                <th>Vendor Name</th>
                                <th>Vendor Place</th>
                                <th>Original Amount(MPRP)</th>
                                <th>Cost</th>
                                <th>Discount</th>
                                <th>Bill No</th>
                                <th>No of Copies</th>
                                <th>Bill Date</th>
                                <th>Class No</th>
                                <th>Book Number</th>
                                <th>Collection No</th>
                                <th>ISBN Number</th>
                                <th>Location</th>
                                <th>Rack No</th>
                                <th>Shelf No</th>
                                <th>Language</th>
                                <th>Series</th>
                                <th>Issue No</th>
                                <th>Withdrawal On</th>
                                <th>Custom Tags</th>
                                <th>Remarks</th>
                                <th>Subject Code</th>
                                <th>Pages</th>
                                <th>Month and Year</th>
                                <th>Source</th>
                                <th>Reference</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                            
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="dvAddEditBookPopUp" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        @await Html.PartialAsync("_addBook")
    </div>
</div>
