@page
@model List<LMS.Infrastructures.Models.StudentContact>
@{
}
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Address 1</th>
                                <th>Address 2</th>
                                <th>Address 3</th>
                                <th>City</th>
                                <th>District</th>
                                <th>State</th>
                                <th>Pin Code</th>
                                <th>Country</th>
                                <th>Mobile</th>
                                <th>Father's Mobile</th>
                                <th>Mother's Mobile</th>
                                <th>Email</th>
                                <th>Alternate Email</th>
                                <th>Student ID</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null)
                            {
                                @foreach (var student in Model)
                                {
                                    <tr>
                                        <td>@student.Address1</td>
                                        <td>@student.Address2</td>
                                        <td>@student.Address3</td>
                                        <td>@student.City</td>
                                        <td>@student.District</td>
                                        <td>@student.State</td>
                                        <td>@student.PinCode</td>
                                        <td>@student.Country</td>
                                        <td>@student.Mobile</td>
                                        <td>@student.FatherMobile</td>
                                        <td>@student.MotherMobile</td>
                                        <td>@student.Email</td>
                                        <td>@student.AlternateEmail</td>
                                        <td>@student.StudentId</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
