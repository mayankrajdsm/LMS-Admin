@page
@model List<LMS.Infrastructures.Models.StudentEducation>
@{
}
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>School Name</th>
                                <th>School Address</th>
                                <th>Affiliated To</th>
                                <th>Class</th>
                                <th>Year</th>
                                <th>Marks</th>
                                <th>Percentage</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null)
                            {
                                @foreach (var education in Model)
                                {
                                    <tr>
                                        <td>@education.SchoolName</td>
                                        <td>@education.SchoolAddress</td>
                                        <td>@education.SchoolAffiliatedTo</td>
                                        <td>@education.Class</td>
                                        <td>@education.Year</td>
                                        <td>@education.Marks</td>
                                        <td>@education.Percentage</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

